{
    "name": "butschster/meta-tags",
    "description": "The most powerful and extendable tools for managing SEO Meta Tags in your Laravel project",
    "keywords": [
        "laravel5",
        "lumen",
        "seo",
        "webmaster",
        "metatags",
        "opengraph"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Pavel Buchnev",
            "email": "butschster@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "php": ">=8.0",
        "illuminate/config": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "illuminate/contracts": "^9.0|^10.0",
        "illuminate/console": "^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^9.3",
        "vimeo/psalm": "^5.15"
    },
    "autoload": {
        "psr-4": {
            "Butschster\\Head\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Butschster\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Butschster\\Head\\Providers\\MetaTagsServiceProvider"
            ],
            "aliases": {
                "Meta": "Butschster\\Head\\Facades\\Meta",
                "PackageManager": "Butschster\\Head\\Facades\\PackageManager"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
